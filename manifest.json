{
	"manifest_version": 3,
	"name": "Shark Tank Now",
	"version": "1.0",
	"permissions": ["activeTab", "scripting"],
	"action": {
		"default_popup": "popup.html"
	},
	"background": {
		"service_worker": "background.js"
	},
	"content_scripts": [
		{
			"matches": ["*://*.youtube.com/*"],
			"js": ["content.js"]
		}
	]
}
